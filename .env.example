# Application runtime
APP_ENV=development
PORT=3000

# Performance budgets (optional overrides)
PERF_REPORT_P50_MS=300
PERF_REPORT_P95_MS=600
PERF_REPORT_HARD_LIMIT_MS=1000
PERF_REPORT_MAX_PAGE_SIZE=200

# Webhook hardening (optional overrides)
WEBHOOK_RATE_LIMIT_PER_MINUTE=5
WEBHOOK_RATE_LIMIT_WINDOW_MS=60000
WEBHOOK_CIRCUIT_FAILURE_THRESHOLD=3
WEBHOOK_CIRCUIT_COOLDOWN_MS=30000

# Retention scheduler (optional override)
RETENTION_PURGE_INTERVAL_MS=3600000

# Supabase runtime + CI migration gate contract
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_DB_URL=postgresql://postgres@db.your-project-ref.supabase.co:5432/postgres?sslmode=require
SUPABASE_DB_PASSWORD=your-supabase-db-password

# Vercel production deploy (GitHub Actions)
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id
